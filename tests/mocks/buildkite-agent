#!/bin/bash
case "$1" in
  "meta-data")
    case "$2" in
      "get") 
        # Return empty for metadata that doesn't exist
        echo ""
        exit 1
        ;;
      "set") 
        echo "Metadata set"
        exit 0
        ;;
      *) echo "meta-data: $*" ;;
    esac
    ;;
  "annotate") 
    echo "Annotation created"
    exit 0
    ;;
  "artifact") 
    echo "Artifact uploaded"
    exit 0
    ;;
  "pipeline") 
    echo "Pipeline uploaded"
    exit 0
    ;;
  *) echo "buildkite-agent: $*" ;;
esac
